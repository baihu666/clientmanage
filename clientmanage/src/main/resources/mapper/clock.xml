<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.dao.IClockDao">
	<select id="findAll" resultType="com.company.pojo.Clock">
		select * from `clock`
	</select>
	<select id="findById" resultType="com.company.pojo.Clock">
		select * from `clock` where id = #{id}
	</select>
</mapper>